function somaMatriz(matriz) {
  let somaTotal = 0;

  for (let i = 0; i < matriz.length; i++) {
    for (let j = 0; j < matriz[i].length; j++) {
      somaTotal += matriz[i][j];
    }
  }

  return somaTotal;
}

const matriz = [
  [7, 1, 12],
  [8, 5, 5],
  [6, 4, 9]
];

const resultado = somaMatriz(matriz);
console.log(resultado);
